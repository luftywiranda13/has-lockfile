{
  "name": "has-lockfile",
  "version": "2.0.0",
  "description": "Detect lockfiles in the working directory",
  "license": "MIT",
  "repository": "https://github.com/luftywiranda13/has-lockfile",
  "author": {
    "name": "Lufty Wiranda",
    "email": "lufty.wiranda@gmail.com",
    "url": "https://www.luftywiranda.com"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "precommit": "remove-lockfiles && xo --fix && lint-staged",
    "lint": "xo",
    "pretest": "remove-lockfiles",
    "test:watch": "jest --watch",
    "test": "jest --coverage"
  },
  "files": [
    "index.js"
  ],
  "dependencies": {
    "has-package-lock": "^1.1.0",
    "has-shrinkwrap": "^1.0.0",
    "has-yarn": "^1.0.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "remove-lockfiles": "^1.4.2",
    "xo": "^0.18.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "xo": {
    "extends": "prettier",
    "envs": [
      "jest"
    ]
  },
  "keywords": [
    "yarn",
    "lockfile",
    "lock file",
    "package-lock",
    "has",
    "detect",
    "is",
    "project",
    "app",
    "module",
    "package",
    "manager",
    "npm"
  ]
}
